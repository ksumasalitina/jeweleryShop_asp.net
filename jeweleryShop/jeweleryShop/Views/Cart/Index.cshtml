@model ShopCartViewModel
<div class="container">
    @foreach(var el in Model.shopCart.cartItems)
            {
                <div class="alert alert-warning mt-3">
                    <b>Product: </b>@el.product.name<br />
                    <b>Price: $</b>@el.product.price.ToString();

                </div>
            }
      <hr />
      <a class="btn btn-danger" asp-controller="Order" asp-action="Checkout">Оформить</a>
</div>